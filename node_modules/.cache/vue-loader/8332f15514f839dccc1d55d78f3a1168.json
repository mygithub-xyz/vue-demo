e: \"Book.vue\",\r\n        data() {\r\n            return {\r\n                list: [],\r\n                entity: {},\r\n            }\r\n        },\r\n        mounted: function () {\r\n            this.findAll();\r\n        },\r\n        methods: {\r\n            findAll(){\r\n                axios.get('http://localhost:8081/people/findAll').then(function (resp) {\r\n                    var _data = resp.data;\r\n                    this.list = _data;\r\n                })\r\n            },\r\n            //增\r\n            add() {\r\n                const _this = this;\r\n                _this.entity = {\r\n                };\r\n                this.editDialogVisible = true;\r\n                axios.post('http://localhost:8081/people/add?people='+_this.people).then(function (resp) {\r\n                    alert(resp.success);\r\n                })\r\n            },\r\n            //删除组件，必须获取对应的下标索引\r\n            del(index) {\r\n                axios.delete('http://localhost:8081/people/delete?selectIds='+index).then(function (resp) {\r\n                    alert(resp.success);\r\n                })\r\n            },\r\n            edit(item) {\r\n                this.editDialogVisible = true;\r\n                this.entity = {\r\n                    pname: item.pname,\r\n                    countryid: item.countryid,\r\n                    createtime: item.createtime, //重新指向\r\n                    pid: item.pid\r\n                };\r\n                //更新  生拷贝\r\n            },\r\n            //改\r\n            change() {\r\n              this.findAll();\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    td {\r\n        width: 400px;\r\n    }\r\n</style>\r\n"]}]}