<a @click=\"change\">修改</a>|\r\n                        <a @click=\"del(entity.pid)\">删除</a>|\r\n                        <a @click=\"add\">添加</a>\r\n                    </td>\r\n                </tr>\r\n                </tbody>\r\n            </table>\r\n\r\n        </div>\r\n    <!-- /.box-body -->\r\n\r\n    <!-- 编辑窗口 -->\r\n                <div >\r\n                    <table >\r\n                        <tr>\r\n                            <td>姓名</td>\r\n                            <td><input type=\"text\" placeholder=\"姓名\" v-model=\"people.pname\"></td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>身份证号码</td>\r\n                            <td><input type=\"text\" placeholder=\"身份证号码\" v-model=\"people.countryid\"></td>\r\n                        </tr>\r\n                        <tr v-if=\"people.pid==null\">\r\n                            <td>创建时间</td>\r\n                            <td ></td>\r\n                        </tr>\r\n                    </table>\r\n                </div>\r\n                <div>\r\n                    <td><input type=\"button\" @click=\"save()\">保存</td>\r\n                    <td><a >关闭</a></td>\r\n                </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Book.vue\",\r\n        data() {\r\n            return {\r\n                list: [],\r\n                flag: false, //弹窗的显示隐藏\r\n                entity: {},\r\n            }\r\n        },\r\n        created() {\r\n                const _this = this;\r\n                axios.get('http://localhost:8081/people/findAll').then(function (resp) {\r\n                    _this.list = resp.data;\r\n                })\r\n            },\r\n        methods: {\r\n            //增\r\n            add() {\r\n                const _this = this;\r\n                _this.entity = {\r\n                    pname: \"\",\r\n                    countryid: \"\",\r\n                    createtime: '',\r\n                    pid: ''\r\n                };\r\n                alert(_this.entity);\r\n                axios.post('http://localhost:8081/people/add?people='+_this.people).then(function (resp) {\r\n                   alert(resp.success);\r\n                })\r\n            },\r\n            //删除组件，必须获取对应的下标索引\r\n            del(index) {\r\n                axios.delete('http://localhost:8081/people/delete?selectIds='+index).then(function (resp) {\r\n                    alert(resp.success);\r\n                })\r\n            },\r\n            //编辑组件 查   item 循环对应的每一个值 与他内容一一对应\r\n            edit(item) {\r\n                //就是显示弹层\r\n                // this.editdetail = item;//对象赋值是引用关系\r\n                this.flag = true; //弹层\r\n                // 为了防止连动，对象赋值是引用关系，原有发生变化是她会随着变化\r\n                // 解决办法\r\n                this.entity = {\r\n                    pname: item.pname,\r\n                    countryid: item.countryid,\r\n                    createtime: item.createtime, //重新指向\r\n                    pid: item.pid\r\n                };\r\n                //更新  生拷贝\r\n            },\r\n            //改\r\n            change() {\r\n                //数据更新\r\n                for (var i = 0; i < this.infos.length; i++) {\r\n                    if (this.list[i].pid == this.people.pid) {\r\n                        this.list[i] = this.people;\r\n                        console.log(this.list[i].pid);\r\n                        console.log(this.entity);\r\n\r\n\r\n                        this.flag = false; //弹窗\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        }\r\n</script>\r\n\r\n<style scoped>\r\n    td {\r\n        width: 400px;\r\n    }\r\n</style>\r\n"]}]}